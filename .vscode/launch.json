{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "C#: MyProject Debug",
        "type": "dotnet",
        "request": "launch",
        "projectPath": "${workspaceFolder}/<relative-path-to-project-folder><project-name>.csproj"
      },
      {
        "name": ".NET Core Attach",
        "type": "coreclr",
        "request": "attach"
      },
      


        {
            "name": ".NET Core Launch",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/debug/pwsh",
            "preLaunchTask": "Build",
            "cwd": "${workspaceRoot}"
        },
        {
            "name": ".NET Core Attach",
            "type": "node",
            "request": "attach",
            "processId": "${command:pickProcess}"
        },
        {
            "type": "PowerShell",
            "request": "launch",
            "name": "PowerShell Launch Current File",
            "script": "${file}",
            "args": [],
            "cwd": "${file}"
        },
        {
            "type": "PowerShell",
            "request": "launch",
            "name": "PowerShell Launch Current File w/Args Prompt",
            "script": "${file}",
            "args": [
                "${command:SpecifyScriptArgs}"
            ],
            "cwd": "${file}"
        }
    ]
}
